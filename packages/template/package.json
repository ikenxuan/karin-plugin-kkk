{
  "name": "template",
  "version": "0.0.1",
  "description": "React模板渲染服务包",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "development": "./src/server.ts",
      "import": "./dist/server.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dev:core": "cross-env RENDER_ENV=core_dev tsx watch src/server.ts",
    "dev:mock": "cross-env RENDER_ENV=core_dev LOG_LEVEL=debug tsx watch src/dev/mock-server.ts",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "sort": "sort-package-json",
    "start": "cross-env RENDER_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@heroui/chip": "^2.2.21",
    "@heroui/react": "^2.8.3",
    "@heroui/system": "^2.4.21",
    "@heroui/theme": "^2.4.21",
    "@shopify/draggable": "^1.1.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.10.0",
    "cross-env": "10.0.0",
    "framer-motion": "^12.23.12",
    "qrcode": "^1.5.4",
    "react-resizable-panels": "^3.0.5"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "concurrently": "^9.2.1",
    "node-karin": "1.10.20",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-plugin-svgr": "^4.5.0"
  }
}