{
  "name": "karin-plugin-kkk-template",
  "version": "0.0.1",
  "description": "React SSR 模板开发框架 - 用于开发和构建 Karin 插件模板",
  "author": "ikenxuan",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./cli": {
      "types": "./dist/cli.d.mts",
      "import": "./dist/cli.mjs"
    }
  },
  "bin": {
    "kkt": "./dist/cli.mjs",
    "karin-template": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "client",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown && vite build",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@heroui/react": "2.8.5",
    "@heroui/system": "2.4.23",
    "@heroui/theme": "2.4.23",
    "@tailwindcss/vite": "4.1.17",
    "@zumer/snapdom": "^2.0.1",
    "cac": "^6.7.14",
    "chokidar": "^5.0.0",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "defu": "^6.1.4",
    "esbuild": "^0.27.1",
    "fast-glob": "^3.3.3",
    "framer-motion": "^12.4.10",
    "gsap": "^3.13.0",
    "jiti": "^2.4.2",
    "lucide-react": "^0.555.0",
    "picocolors": "^1.1.1",
    "react-dev-inspector": "^2.0.1",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^3.0.6",
    "react-scan": "^0.4.3",
    "tailwindcss": "4.1.17",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "tsdown": "0.16.8",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "karin",
    "template",
    "react",
    "ssr",
    "framework"
  ]
}
