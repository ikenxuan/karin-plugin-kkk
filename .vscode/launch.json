      "webRoot": "${workspaceFolder}/packages/template/*",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "/*": "${workspaceFolder}/packages/template/*"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Test Docs",
      "url": "http://localhost:5175",
      "webRoot": "${workspaceFolder}/packages/docs/*",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "/*": "${workspaceFolder}/packages/docs/*"
      }
    }
  ]
}