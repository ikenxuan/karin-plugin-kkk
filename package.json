{
  "name": "karin-plugin-kkk",
  "version": "0.1.6",
  "description": "a Karin video parsing tool",
  "keywords": [
    "karin-plugin",
    "karin"
  ],
  "homepage": "https://github.com/ikenxuan/karin-plugin-kkk",
  "bugs": {
    "url": "https://github.com/ikenxuan/karin-plugin-kkk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenxuan/karin-plugin-kkk.git"
  },
  "license": "GPL-3.0-only",
  "author": "ikenxuan",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "fix": "eslint src/**/*.ts --fix",
    "pr": "node lib/cli/pr.js",
    "pub": "npm publish --access public",
    "pub-beta": "npm publish --tag beta",
    "sort": "npx sort-package-json"
  },
  "dependencies": {
    "@ikenxuan/amagi": "^3.4.4",
    "@karinjs/md-html": "^1.1.6",
    "axios": "^1.7.9",
    "heic-convert": "^2.1.0",
    "playwright": "^1.49.0",
    "qrcode": "^1.5.4",
    "sequelize": "6.37.3",
    "sqlite3": "5.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/heic-convert": "^2.1.0",
    "@types/node": "^22.10.1",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.13.0",
    "neostandard": "^0.11.9",
    "node-karin": "0.12.25-beta.pr.206.1-1734649344",
    "sort-package-json": "^2.12.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ]
  }
}