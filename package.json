{
  "name": "karin-plugin-kkk",
  "version": "1.8.1",
  "description": "Karin 的「抖音」「B 站」视频解析/动态推送插件",
  "keywords": [
    "karin-plugin",
    "karin"
  ],
  "homepage": "https://github.com/ikenxuan/karin-plugin-kkk",
  "bugs": {
    "url": "https://github.com/ikenxuan/karin-plugin-kkk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenxuan/karin-plugin-kkk.git"
  },
  "license": "GPL-3.0-only",
  "author": "ikenxuan",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./template": {
      "types": "./lib/core_chunk/modules.d.ts",
      "import": "./lib/core_chunk/modules.js"
    }
  },
  "main": "lib/index.js",
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production vite build && tsup",
    "dev": "cross-env EBV_FILE=\"development.env\" node --import tsx src/app.ts --trace-deprecation",
    "dev:template": "cross-env RENDER_ENV=core_dev tsx ../render/src/main.ts",
    "lint": "eslint . --fix",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "sync": "curl -X PUT \"https://registry-direct.npmmirror.com/-/package/karin-plugin-kkk/syncs\"",
    "watch": "cross-env EBV_FILE=\"development.env\" tsx watch --include \"src/**/*.ts\" --include \"../render/src/dev/mock-server.ts\" --include \"../render/src/main.ts\" src/app.ts --trace-deprecation"
  },
  "dependencies": {
    "@karinjs/md-html": "^1.3.0"
  },
  "engines": {
    "node": ">=21.0.0",
    "pnpm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts"
  }
}
