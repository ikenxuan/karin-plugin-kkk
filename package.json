{
  "name": "karin-plugin-kkk",
  "version": "1.2.0",
  "description": "Karin 的「抖音」「B 站」视频解析/动态推送插件",
  "keywords": [
    "karin-plugin",
    "karin"
  ],
  "homepage": "https://github.com/ikenxuan/karin-plugin-kkk",
  "bugs": {
    "url": "https://github.com/ikenxuan/karin-plugin-kkk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenxuan/karin-plugin-kkk.git"
  },
  "license": "GPL-3.0-only",
  "author": "ikenxuan",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "web.config.mjs"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "clean": "npm lib/cli/pr.js clean",
    "commit": "node lib/cli/commit.js",
    "dev": "tsx src/index.ts",
    "fix": "eslint src/**/*.ts --fix",
    "pr": "node lib/cli/pr.js",
    "pub": "npm publish --access public",
    "pub-beta": "npm publish --tag beta",
    "sort": "npx sort-package-json",
    "watch": "tsx watch --include \"src/**/*.ts\" src/index.ts",
    "sync": "curl -X PUT \"https://registry-direct.npmmirror.com/-/package/karin-plugin-kkk/syncs\""
  },
  "dependencies": {
    "@ikenxuan/amagi": "^4.4.8",
    "@karinjs/md-html": "^1.1.6",
    "cors": "^2.8.5",
    "heic-convert": "^2.1.0",
    "http-proxy-middleware": "^3.0.3",
    "playwright": "^1.51.1",
    "qrcode": "^1.5.4",
    "sequelize": "6.37.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@karinjs/plugin-puppeteer": "^1.0.6",
    "@types/cors": "^2.8.17",
    "@types/heic-convert": "^2.1.0",
    "@types/node": "^22.13.14",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "neostandard": "^0.11.9",
    "node-karin": "^1.7.14",
    "sort-package-json": "^2.15.1",
    "tsc-alias": "^1.8.12",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts"
  }
}